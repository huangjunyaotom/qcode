<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    
    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />
    

    <package name="huang" extends="struts-default">
    	<action name="*">
    		<result>/WEB-INF/jsp/{1}.jsp</result>
    	</action>
    	<action name="qcode" class="qcode.Qcode">
    		<result name="show">/WEB-INF/jsp/show.jsp</result>
    		
    		<result name="upfile">/WEB-INF/jsp/upfile.jsp</result>
    		<result name="none">/WEB-INF/jsp/none.jsp</result>
    	</action>
    	
    </package>

    

</struts>
